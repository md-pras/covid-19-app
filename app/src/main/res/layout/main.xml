<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="150dp"
		android:padding="8dp"
		android:background="@drawable/covid19a"
		android:gravity="right|top"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/globe_3"
			android:scaleType="center"
			android:scaleX="0.9"
			android:scaleY="0.9"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bg_title"
		android:layout_width="match_parent"
		android:layout_height="60dp"
		android:layout_marginLeft="25dp"
		android:layout_marginRight="25dp"
		android:background="#F5F5F5"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal"
		android:translationY="-30.0dp">
		<TextView
			android:id="@+id/textview12"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:text="Kasus Covid-19 di Indonesia"
			android:textSize="18sp"
			android:textStyle="bold"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bg_yellow"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="40dp"
		android:layout_marginRight="40dp"
		android:layout_marginBottom="15dp"
		android:padding="8dp"
		android:background="#F6C23E"
		android:gravity="center_horizontal"
		android:orientation="vertical">
		<TextView
			android:id="@+id/text_pos"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:paddingBottom="8dp"
			android:text="0"
			android:textSize="30sp"
			android:textStyle="bold"
			android:textColor="#F5F5F5"/>
		<TextView
			android:id="@+id/textview5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:text="Positif"
			android:textSize="16sp"
			android:textColor="#F5F5F5"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bg_green"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="40dp"
		android:layout_marginTop="10dp"
		android:layout_marginRight="40dp"
		android:layout_marginBottom="15dp"
		android:padding="8dp"
		android:background="#1CC88A"
		android:gravity="center_horizontal"
		android:orientation="vertical">
		<TextView
			android:id="@+id/text_sem"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:paddingBottom="8dp"
			android:text="0"
			android:textSize="30sp"
			android:textStyle="bold"
			android:textColor="#F5F5F5"/>
		<TextView
			android:id="@+id/textview7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:text="Sembuh"
			android:textSize="16sp"
			android:textColor="#F5F5F5"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bg_red"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="40dp"
		android:layout_marginTop="10dp"
		android:layout_marginRight="40dp"
		android:layout_marginBottom="15dp"
		android:padding="8dp"
		android:background="#E74A3B"
		android:gravity="center_horizontal"
		android:orientation="vertical">
		<TextView
			android:id="@+id/text_men"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:paddingBottom="8dp"
			android:text="0"
			android:textSize="30sp"
			android:textStyle="bold"
			android:textColor="#F5F5F5"/>
		<TextView
			android:id="@+id/textview9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:text="Meninggal"
			android:textSize="16sp"
			android:textColor="#F5F5F5"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear6"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_margin="10dp"
		android:paddingLeft="20dp"
		android:paddingTop="10dp"
		android:paddingRight="20dp"
		android:paddingBottom="10dp"
		android:background="#CFD8DC"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="horizontal"
		android:layout_gravity="center_horizontal">
		<TextView
			android:id="@+id/textview10"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Kasus tiap provinsi"
			android:textSize="18sp"
			android:textColor="#000000"
			android:layout_weight="1"/>
		<TextView
			android:id="@+id/textview11"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:text="&gt;&gt;"
			android:textSize="18sp"
			android:textColor="#000000"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/linear7"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:gravity="bottom"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/nav"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:paddingTop="8dp"
				android:background="#4E73DF"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/globe_4"
					android:scaleType="center"
					android:scaleX="0.9"
					android:scaleY="0.9"
					android:layout_weight="1"/>
				<ImageView
					android:id="@+id/line"
					android:layout_width="match_parent"
					android:layout_height="4dp"
					android:layout_marginTop="5dp"
					android:background="#FFFFFF"
					android:src="@drawable/line2"
					android:scaleType="center"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:paddingTop="8dp"
				android:paddingBottom="8dp"
				android:background="#3584FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/globe_1"
					android:scaleType="center"
					android:scaleX="0.9"
					android:scaleY="0.9"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:background="#3584FB"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical"
				android:layout_weight="1">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:src="@drawable/globe_2"
					android:scaleType="center"
					android:scaleX="0.9"
					android:scaleY="0.9"/>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
